{
  "name": "root-test01",
  "version": "1.0.0",
  "description": "host application",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "tsc && webpack serve",
    "start:uat": "tsc && webpack serve --env isLocal --env rp-env=uat",
    "start:prod": "tsc && webpack serve --env isLocal --env rp-env=prod",
    "build": "tsc && webpack --mode=production --env prod=false",
    "build:uat": "tsc && webpack --mode=production --env prod=false --env rp-env=uat",
    "build:prod": "tsc && webpack --mode=production --env prod=true --env rp-env=prod",
    "start:locally": "tsc && webpack --mode=production --env isLocal && pm2 serve dist 8035 --name ui-mf-test01 --spa",
    "stop:locally": "pm2 delete ui-mf-test01"
  },
  "devDependencies": {
